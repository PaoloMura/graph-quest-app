[
  {
    "selector": "node",
    "style": {
      "background-color": "#000",
      "width": 15,
      "height": 15
    }
  },
  {
    "selector": "node.ring",
    "style": {
      "background-color": "#fff",
      "border-style": "solid",
      "border-width": 2,
      "width": 30,
      "height": 30,
      "text-valign": "center",
      "text-halign": "center"
    }
  },
  {
    "selector": "node.blank",
    "style": {
      "background-opacity": 0,
      "border-opacity": 0,
      "padding": 0,
      "text-valign": "center",
      "text-halign": "center"
    }
  },
  {
    "selector": "node.box",
    "style": {
      "background-color": "#fff",
      "border-style": "solid",
      "border-width": 2,
      "shape": "round-rectangle",
      "padding": 0
    }
  },
  {
    "selector": ".label",
    "style": {
      "label": "data(label)",
      "font-size": 25
    }
  },
  {
    "selector": ".styled-label",
    "style": {
      "font-style": "italic",
      "font-family": "serif"
    }
  },
  {
    "selector": "edge",
    "style": {
      "width": 3,
      "line-color": "#000",
      "curve-style": "bezier",
      "background-color": "#fff"
    }
  },
  {
    "selector": ".weighted",
    "style": {
      "text-background-color": "#fff",
      "text-background-opacity": 1,
      "text-border-color": "#fff",
      "text-border-width": 5,
      "text-border-opacity": 1,
      "label": "data(weight)"
    }
  },
  {
    "selector": ".directed",
    "style": {
      "target-arrow-color": "#000",
      "target-arrow-shape": "triangle"
    }
  },
  {
    "selector": "edge.colour",
    "style": {
      "line-color": "#c00",
      "source-arrow-color": "#c00",
      "target-arrow-color": "#c00",
      "width": 7,
      "line-style": "dashed",
      "line-dash-pattern": [
        20,
        10
      ]
    }
  },
  {
    "selector": "node.colour",
    "style": {
      "background-color": "#c00",
      "width": 25,
      "height": 25
    }
  },
  {
    "selector": "node:active",
    "style": {
      "overlay-shape": "ellipse"
    }
  },
  {
    "selector": ".underlay",
    "style": {
      "underlay-opacity": 0.3,
      "underlay-color": "#00f",
      "underlay-padding": 5
    }
  },
  {
    "selector": "node.underlay",
    "style": {
      "underlay-shape": "ellipse"
    }
  }
]