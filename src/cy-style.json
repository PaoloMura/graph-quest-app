[
  {
    "selector": "node",
    "style": {
      "background-color": "#000",
      "width": 30,
      "height": 30
    }
  },
  {
    "selector": "node.ring",
    "style": {
      "border-style": "solid",
      "border-width": 5,
      "text-valign": "center",
      "text-halign": "center",
      "padding": 10
    }
  },
  {
    "selector": "node.blank",
    "style": {
      "background-opacity": 0,
      "border-opacity": 0,
      "padding": 0,
      "text-valign": "center",
      "text-halign": "center"
    }
  },
  {
    "selector": "node.box",
    "style": {
      "background-color": "#fff",
      "border-style": "solid",
      "border-width": 5,
      "shape": "round-rectangle",
      "padding": 5
    }
  },
  {
    "selector": "node.label",
    "style": {
      "label": "data(label)",
      "font-size": 40
    }
  },
  {
    "selector": "node.styled-label",
    "style": {
      "font-style": "italic",
      "font-family": "serif"
    }
  },
  {
    "selector": "node:active",
    "style": {
      "overlay-shape": "ellipse"
    }
  },
  {
    "selector": "node.colour",
    "style": {
      "background-color": "data(colour)"
    }
  },
  {
    "selector": "edge",
    "style": {
      "width": 6,
      "line-color": "#000",
      "curve-style": "bezier",
      "background-color": "#fff"
    }
  },
  {
    "selector": "edge.weighted",
    "style": {
      "text-background-color": "#fff",
      "text-background-opacity": 1,
      "text-border-color": "#fff",
      "text-border-width": 5,
      "text-border-opacity": 1,
      "label": "data(weight)",
      "font-size": 30
    }
  },
  {
    "selector": "edge.directed",
    "style": {
      "target-arrow-color": "#000",
      "target-arrow-shape": "triangle",
      "arrow-scale": 2
    }
  },
  {
    "selector": "edge.colour",
    "style": {
      "line-color": "#c00",
      "source-arrow-color": "#c00",
      "target-arrow-color": "#c00",
      "width": 10,
      "line-style": "dashed",
      "line-dash-pattern": [
        20,
        10
      ]
    }
  },
  {
    "selector": ".underlay",
    "style": {
      "underlay-opacity": 0.3,
      "underlay-color": "#00f",
      "underlay-padding": 12,
      "underlay-shape": "ellipse"
    }
  },
  {
    "selector": ".overlay",
    "style": {
      "overlay-opacity": 0.3,
      "overlay-color": "#f00",
      "overlay-padding": 10,
      "overlay-shape": "ellipse"
    }
  }
]